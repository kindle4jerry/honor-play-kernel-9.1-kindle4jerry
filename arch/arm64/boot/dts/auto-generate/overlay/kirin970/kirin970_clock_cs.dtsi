					compatible = "hisilicon,ppll-ctrl";
					#clock-cells = <0>;
					clocks = <&clk_ppll1>;
					hisilicon,ipc-lpm3-cmd-en = <0x000D0002 0x1>;
					hisilicon,ipc-lpm3-cmd-dis = <0x000D0102 0x1>;
