					compatible = "fixed-factor-clock";
					#clock-cells = <0>;
					clock-fpga = "clk_fpga_20m";
					clocks = <&clk_mux_timer3_a>;
					clock-div = <1>;
					clock-mult = <1>;
					clock-output-names = "clk_timer3_a";
					clock-friend-names = "clk_timer3";
