			reg = <0xE86CB000 0x800>;
			eprobe-hwirq = <18>;
			hwirq-type = <2>;
			eprobe-offset = <0x0>;
			eprobe-autoenable;
			pwrack-bit = <13>;
		};

		isp_rd_transcation_bus@E86D8000 {
			compatible = "hisilicon,noc-node";
			bus-name = "vivo_bus";
