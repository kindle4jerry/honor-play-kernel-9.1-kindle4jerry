			pwrack-bit = <1>;
			crg_clk0=<0xc 17>;
			crg_clk1=<0xc 18>;
		};

		mmc1_bus@E9880000 {
			compatible = "hisilicon,noc-node";
			bus-name = "cfg_sys_noc_bus";
			reg = <0xE9880000 0x2000>;
			eprobe-hwirq = <9>;
			hwirq-type = <0>;
			eprobe-offset = <0x1C00>;
			eprobe-autoenable;
			pwrack-bit = <2>;
			crg_clk0=<0xc 14>;
			crg_clk1=<0xc 15>;
		};

		ivp32_bus@E9850000 {
			compatible = "hisilicon,noc-node";
			bus-name = "cfg_sys_noc_bus";
			reg = <0xE9850000 0x2000>;
			eprobe-hwirq = <10>;
			hwirq-type = <0>;
			eprobe-offset = <0x1C00>;
			eprobe-autoenable;
			pwrack-bit = <14>;
			crg_clk0=<0xc 10>;
			crg_clk1=<0xFFFFFFFF 0>; /* ignore this clk-bit*/
		};

		modem_transcation_bus@E9828000 {
			compatible = "hisilicon,noc-node";
