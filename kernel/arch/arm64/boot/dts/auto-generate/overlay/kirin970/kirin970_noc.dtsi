			pwrack-bit = <1>;
			crg_clk0=<0x0c 17>;
			crg_clk1=<0x0c 18>;
			crg_clk2=<0x5c 19>;
		};

		mmc1_bus@E9880000 {
			compatible = "hisilicon,noc-node";
