			eprobe-offset = <0x0>;
			eprobe-autoenable;
			pwrack-bit = <3>;
		};

		dss0_rd_transcation_bus@E86C8000 {
			compatible = "hisilicon,noc-node";
			bus-name = "vivo_bus";
			reg = <0xE86C8000 0x800>;
