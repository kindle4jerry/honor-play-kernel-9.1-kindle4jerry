			eprobe-autoenable;
			pwrack-bit = <2>;
			crg_clk0=<0x0c 14>;
			crg_clk1=<0x0c 15>;
			crg_clk2=<0x5c 20>;
		};

		modem_transcation_bus@E9828000 {
			compatible = "hisilicon,noc-node";
			bus-name = "cfg_sys_noc_bus";
			reg = <0xE9828000 0x800>;
